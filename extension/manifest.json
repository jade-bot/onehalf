{
  "name": "Nibun No Ichi",
  "version": "0.1",
  "description": "Browser Sync",
  "background_page": "background_page.html",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "enable.png",
    "default_title": "もっとひっついてたいのさ"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/frontend.js"],
      "run_at": "document_start"
    }
  ]
}
